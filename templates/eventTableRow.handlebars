<li id="{{eventId}}" class="event-row list-group-item " data-Main_State-filter={{State}} data-Member-filter={{Member}}
  data-toggle="collapse" data-target="#notes-{{eventId}}" title="Click to toggle notes">
  <span class="member">
    <h4>
    {{Member}}
    <small>({{Party}})  {{State}},   {{District}}
    </small>
  </h4>
 <span class="badge badge-default badge-pill pull-right">  {{meetingType}}</span>
  </span>
  <ul class="list-inline list-inline-separated">
    {{#if Date}}
    <li>{{Date}}
    </li>
    {{/if}}
    {{#if Time}}
    <li>{{Time}}{{#if timeZone}}, {{timeZone}}{{/if}}  </li>
    {{/if}}
    {{#if eventName}}
    <li>{{eventName}}</li>
    {{/if}}
    {{#if Location}}
    <li>{{Location}}</li>
    {{/if}}
    {{#if phoneNumber}}
    <li>{{phoneNumber}}</li>
    {{/if}}
    {{#if address}}
    <li>{{address}}</li>
    {{/if}}
  </ul>
  <div id="notes-{{eventId}}" class="collapse notes">{{#if Notes}}{{{Notes}}}{{/if}}{{#unless Notes}}There are no notes for this event.{{/unless}}</div>
</li>
